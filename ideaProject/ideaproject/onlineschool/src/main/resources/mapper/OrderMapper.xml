<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rulaifozu.onlineschool.dao.OrderDao">

    <insert id="addUOrder" parameterType="com.rulaifozu.onlineschool.pojo.UOrder">
        insert into uorder (oId,oMoney,oTime,oType,uInfo_id) values (#{oId},#{oMoney},#{oTime},#{oType},#{uInfo_id.uId})
    </insert>




</mapper>
